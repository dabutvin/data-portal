{
  "name": "mbs-data-portal",
  "version": "0.1.0",
  "description": "Mockingbird Society Data Portal",
  "main": "build/app/main.js",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/mockingbirdsociety/data-portal.git"
  },
  "scripts": {
    "start": "tsc && concurrently \"npm run tsc:w\" \"npm run sass:w\" \"npm run lite\" ",
    "lite": "lite-server",
    "postinstall": "typings install",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "sass:w": "sass --watch assets/sass:assets/css",
    "typings": "typings"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mockingbirdsociety/data-portal/issues"
  },
  "dependencies": {
    "@angular/common": "~2.0.0",
    "@angular/compiler": "~2.0.0",
    "@angular/core": "~2.0.0",
    "@angular/forms": "~2.0.0",
    "@angular/http": "~2.0.0",
    "@angular/platform-browser": "~2.0.0",
    "@angular/platform-browser-dynamic": "~2.0.0",
    "@angular/router": "~3.0.0",
    "angular2-in-memory-web-api": "~0.0.20",
    "bootstrap": "^3.3.7",
    "bootstrap-sass": "^3.3.7",
    "core-js": "^2.4.1",
    "font-awesome": "^4.6.3",
    "reflect-metadata": "^0.1.8",
    "rxjs": "5.0.0-beta.12",
    "systemjs": "0.19.39",
    "zone.js": "^0.6.25"
  },
  "devDependencies": {
    "concurrently": "^3.0.0",
    "lite-server": "^2.2.2",
    "typescript": "^2.0.3",
    "typings": "^1.4.0"
  }
}
